-- Infinite Jump
local infJumpEnabled = false
local infJumpConnection

local InfJumpToggle = createToggleButton(miscButtons, "‚ôæÔ∏è Infinite Jump: OFF")
InfJumpToggle.LayoutOrder = 3
InfJumpToggle.MouseButton1Click:Connect(function()
    infJumpEnabled = not infJumpEnabled
    InfJumpToggle.Text = infJumpEnabled and "‚ôæÔ∏è Infinite Jump: ON" or "‚ôæÔ∏è Infinite Jump: OFF"
    tweenProperty(InfJumpToggle, {BackgroundColor3 = infJumpEnabled and Color3.fromRGB(255, 200, 100) or Color3.fromRGB(50, 50, 70)}, 0.2)
    
    if infJumpEnabled then
        infJumpConnection = UserInputService.JumpRequest:Connect(function()
            local char = player.Character
            if char and char:FindFirstChild("Humanoid") then
                char.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    else
        if infJumpConnection then infJumpConnection:Disconnect() end
    end
end)

